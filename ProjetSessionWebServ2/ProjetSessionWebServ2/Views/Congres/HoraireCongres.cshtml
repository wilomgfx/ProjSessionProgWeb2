@model  IEnumerable<ProjetSessionWebServ2.Models.PlageHoraire>

@{
    ViewBag.Title = ProjetSessionWebServ2.GlobalRessources.CongresRes.CongresSchedule;
}

<h2>@ProjetSessionWebServ2.GlobalRessources.CongresRes.CongresScheduleOrderDesc</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Evenement.Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Evenement.TypeEvenement)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateEtHeureDebut)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateEtHeureFin)
        </th>
        @*<th></th>*@
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Evenement.Nom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Evenement.TypeEvenement)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateEtHeureDebut)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateEtHeureFin)
            </td>
            @*<td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>*@
        </tr>
    }
</table>
